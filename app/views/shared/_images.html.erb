<h4>Explore recently added photos from users!</h4>
<div class ="card-columns">
    <% images.each do |image| %>
        <div class = "card">
            <div class = "card-body p-1">
                <%= image_tag image.image, class: 'd-block w-100 mb-4' %>
                <%= link_to "View Image", image_path(image.id), class: 'btn btn-secondary stretched-link'%>
            </div>
        </div>  
    <% end %>
</div>